<model version="NetLogo 7.0.2" snapToGrid="true">
  <code><![CDATA[turtles-own [
  chips
  believer
  bet-amount
  win-streak
  lose-streak
]

to setup
  clear-all
  
  create-turtles 50 [ set believer "hot-hand" ] 
  create-turtles 50 [ set believer "gambler-reverse" ] 
  create-turtles 50 [ set believer "both" ] 
  create-turtles 50 [ set believer "rational" ] 
  
  ask turtles [
    setxy random-xcor random-ycor
    set shape "person"
    set size 2
    set chips 100
    set bet-amount 1
    set color (ifelse-value
      believer = "hot-hand" [orange]
      believer = "gambler-reverse" [blue]
      believer = "both" [magenta]
      believer = "rational" [green]
    )
    set win-streak 0
    set lose-streak 0
  ]
  reset-ticks
end


to go
  if not any? turtles with [ chips > 0] [stop]
  ask turtles with [chips > 0] [ gambling ]
  tick
end

to gambling
  set bet-amount min list bet-amount chips ;; bet-amount should never above chips of the agent
  let gambling-result (ifelse-value (random 100 < win-rate%) ["Win"] ["Lose"])
  ifelse gambling-result = "Win" [
    set chips chips + bet-amount
    set lose-streak 0
    set win-streak win-streak + 1
    ifelse win-streak = 1 [
      set bet-amount (min list 1 chips)
    ] [
      if ((believer = "hot-hand") or (believer = "both")) and (win-streak >= hot-hand-threshold) [
        if (not cap-hot?) or (win-streak <= maximum-streak-hot) [
          set bet-amount (min list (round (hot-hand-multiplier ^ (win-streak - hot-hand-threshold))) chips)
        ]
      ]
    ]
  ] [
    set chips chips - bet-amount
    set win-streak 0
    set lose-streak lose-streak + 1
    if chips = 0 [ set color color - 3 ]
    ifelse lose-streak = 1 [
      set bet-amount (min list 1 chips)
    ] [ 
      if ((believer = "gambler-reverse") or (believer = "both")) and (lose-streak >= gambler-threshold) [
        if (not cap-gambler?) or (lose-streak <= maximum-streak-gambler) [
          set bet-amount (min list (round (gambler-multiplier ^ (lose-streak - gambler-threshold))) chips)
        ]
      ]
    ]
  ]
end]]></code>
